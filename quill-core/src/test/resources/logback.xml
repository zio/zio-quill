<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n%ex
			</pattern>
		</encoder>
	</appender>

	<!-- <logger name="io.getquill" level="DEBUG" /> -->

	<root level="WARN">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>
